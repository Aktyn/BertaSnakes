{
  "name": "bertasnakes",
  "version": "1.9.9",
  "description": "Web multiplayer game created by single person",
  "main": "src/server/main.js",
  "dependencies": {
    "body-parser": "^1.18.3",
    "canvas": "^1.6.11",
    "fs-watch-tree": "^0.2.5",
    "ip": "^1.1.5",
    "jsdom": "^11.12.0",
    "nodemailer": "^4.6.7",
    "ws": "^5.2.1"
  },
  "devDependencies": {
    "google-closure-compiler-js": "^20180610.0.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "typescript": "^3.1.1"
  },
  "scripts": {
    "dev": "run-p build:live watch:server",
    "watch:server": "nodemon -e js,html --watch src/server --watch website/html src/server/main 2674 1337",
    "test": "nodejs src/server/main 2674 1337",
    "build": "npm run build:website",
    "build:live": "npm run build:website:live",
    "build:website": "echo Compiling website code && tsc -p ./website/. && node ./website/compile",
    "build:website:live": "nodemon -e ts --watch './website/ts/*.ts' --exec tsc -p ./website/.",
    "start": "nodejs src/server/main 2674 80"
  },
  "keywords": [
    "game",
    "web",
    "webgl",
    "javascript",
    "multiplayer",
    "online",
    "browser",
    "berta",
    "snakes"
  ],
