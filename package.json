{
  "name": "bertasnakes",
  "version": "0.8.3",
  "description": "Multilayer browser game created by Aktyn",
  "main": "sources/server/main.ts",
  "scripts": {
    "client:dev": "webpack-dev-server --config ./webpack.config.js",
    "client:build": "webpack --config ./webpack.config.js",
    "client:publish": "rm -rf ./dist/client && NODE_ENV='production' webpack --config ./webpack.config.js -p",
    "server:dev": "NODE_ENV=dev nodemon --watch sources/server/ --watch sources/common/ --exec 'tsc -p ./sources/server/tsconfig.json && node ./dist/server/main.js' -e ts",
    "server:publish": "rm -rf ./dist/server && rm -rf ./dist/common && NODE_ENV=prod tsc -p ./sources/server/tsconfig.json",
    "server:run": "NODE_ENV=prod node dist/server/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aktyn/BertaSnakes.git"
  },
  "keywords": [
    "Berta",
    "Snakes",
    "Multiplayer",
    "Online",
    "Browser"
  ],
  "author": "Aktyn",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Aktyn/BertaSnakes/issues"
  },
  "homepage": "https://github.com/Aktyn/BertaSnakes#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "canvas": "^2.5.0",
    "email-exists": "^0.2.1",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.1-alpha.3",
    "ip": "^1.1.5",
    "mongodb": "^3.2.7",
    "mysql": "^2.16.0",
    "nodemailer": "^5.1.1",
    "prompt-sync": "^4.1.6",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-router-dom": "^4.3.1",
    "ws": "^6.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.1",
    "@types/express-fileupload": "^0.4.0",
    "@types/ip": "^1.1.0",
    "@types/mongodb": "^3.1.28",
    "@types/mysql": "^2.15.5",
    "@types/node": "^10.14.3",
    "@types/nodemailer": "^4.6.8",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@types/react-loadable": "^5.5.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/webpack-env": "^1.13.9",
    "@types/ws": "^6.0.1",
    "autoprefixer": "^9.5.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "css-loader": "^2.1.1",
    "fast-sass-loader": "^1.4.7",
    "file-loader": "^3.0.1",
    "handlebars-loader": "^1.7.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.10",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^3.0.0",
    "react-loadable": "^5.5.0",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.2",
    "typescript": "^3.3.4000",
    "uglifyjs-webpack-plugin": "^2.1.2",
